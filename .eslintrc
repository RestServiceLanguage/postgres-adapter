{
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "quotes": ["error", "single"],
    "arrow-parens": ["error", "always"],
    "no-trailing-spaces": ["error"],
    "semi": ["error"],
    "max-len": ["error", 120],
    "object-curly-spacing": ["error", "always"],
    "indent": ["error", 2]
  }
}
